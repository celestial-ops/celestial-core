{
 :env :dev

 :owner "ronen"

 :machine {
  :hostname "foo" :cpus 4 :memory 4096 :disk 30
 }

 :docker {
   ; used in creation
   :node :local
   :image "narkisr:latest"
   :exposed-ports ["22/tcp"]
   :volumes ["/tmp"]

   ; used in start
   :port-bindings {"22/tcp" [{:host-ip "0.0.0.0" :host-port "2222"}]}
   :binds ["/vagrant:/vagrant"]
 }

 :type "redis"
}
